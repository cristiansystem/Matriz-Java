import java.util.*;
public class Program {
    public static void main(String[] args) {
        Integer[][] matriz = new Integer[3][4];
        Scanner lector = new Scanner(System.in);
        for(int f=0; f<3; f++)
        {
            for(int c=0; c<4; c++)
            {
                System.out.println("Digite numero");
                matriz[f][c] = lector.nextInt();
            }
        }
        
    }
}
